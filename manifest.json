{
    "manifest_version": 2,
    "name": "Cookies fetcher",
    "description": "Get relevant cookies for Panaya login",
    "version": "1.0",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "cookies",
        "tabs", 
        "http://*/*", 
        "https://*/*",
        "activeTab"
    ]
}